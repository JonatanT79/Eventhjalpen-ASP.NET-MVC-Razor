@page
@model EVTHJÄLPEN.Areas.Identity.Pages.Account.Manage.AdressModel
@{
    ViewData["Title"] = "Lägg till adress";
}

<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-6">
        <form id="change-password-form" method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label >Gatuadress</label>
                <input class="form-control" id="Adress" name="Adress"/>
                <span class="text-danger"></span>
            </div>
            <div class="form-group" id="ZipCode">
                <label >Postnummer</label>
                <input class="form-control"id="ZipCode" name="ZipCode" />
                <span  class="text-danger"></span>
            </div>
            <div class="form-group" id="City">
                <label>Stad</label>
                <input class="form-control" id="City" name="City" />
                <span  class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-outline-dark">Spara</button>
        </form>
    </div>
</div>