@model IEnumerable<Events>

<li class="nav-item dropdown">
    <a class="nav-link dropdown-toggle text-light" data-toggle="dropdown" data-target="dropdown_target" asp-area="" asp-controller="Recipe" asp-action="Recipes">Recept</a>
    <div class="dropdown-menu" aria-labelledby="dropdown-target">
        @foreach (var e in Model)
        {
            <a class="dropdown-item text-dark" asp-area="" asp-controller="Recipe" asp-action="RecipesByEvents" asp-route-id="@e.Id">@e.EventType</a>
        }
        <div class="dropdown-divider"></div>
        <a class="dropdown-item text-dark" asp-area="" asp-controller="Recipe" asp-action="RecipesByEvents" asp-route-id="0">Visa alla recept</a>

    </div>
</li>